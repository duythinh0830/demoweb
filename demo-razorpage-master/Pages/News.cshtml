@page
@model NewsWebsite.Pages.NewsModel
@{
    ViewData["Title"] = "News List " + Model.NewsList.Count;
}
<h2>News Listing</h2>
<ui>
    <li>Tin 1</li>
    <li>Tin 2</li>
    <li>Tin 3</li>
</ui>

@for (int i = 0; i < Model.NewsList.Count; i++)
{
    <li>
        <a asp-page="/News/Details" asp-route-id="@(i+1)">@Model.NewsList[i].Title</a>
    </li>
}